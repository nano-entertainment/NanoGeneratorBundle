{
  "name":         "nano-entertainment/generator-bundle",
  "type":         "symfony-bundle",
  "description":  "This bundle generates code based on SensioGeneratorBundle",
  "homepage":     "http://nano-entertainment.de",
  "license":      "MIT",
  "authors": [
    {
      "name":   "Lasse Nielsen",
      "email":  "lasse.nielsen@nano-entertainment.de"
    }
  ],
  "require":  {
    "sensio/generator-bundle": "2.2.*",
    "twig/twig": "1.*"
  },
  "autoload": {
    "psr-0": { "Nano\\Bundle\\GeneratorBundle": "" }
  },
  "target-dir": "Nano/Bundle/GeneratorBundle",
  "minimum-stability": "alpha",
  "extra": {
    "branch-alias": {
      "dev-master": "1.*-dev"
    }
  }
}
